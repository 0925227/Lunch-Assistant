@model EnterProfileViewModel
@{
    ViewData["Title"] = "EnterProfile";
}

<form id="labnol3" class="form" method="post" action="EnterProfile">
    <div class="speech">
        <input asp-for="ProfileInput" class="form-control" />
        @*<input type="text" name="q" id="transcript" placeholder="Speak" />*@
        <img onclick="startDictation3()" src="//i.imgur.com/cHidSVu.gif" />

    </div>
</form>


<div hidden id="page-wrapper">
    <p id="msg"></p>

    <input type="hidden" name="speech-msg" id="speech-msg" value="Spreek uw geslacht, leeftijd en gewicht in" x-webkit-speech>

    <div class="option">
        <label for="voice">Voice</label>
        <select name="voice" id="voice"></select>
    </div>
    <div class="option">
        <label for="volume">Volume</label>
        <input type="range" min="0" max="1" step="0.1" name="volume" id="volume" value="1">
    </div>
    <div class="option">
        <label for="rate">Rate</label>
        <input type="range" min="0.1" max="10" step="0.1" name="rate" id="rate" value="1">
    </div>
    <div class="option">
        <label for="pitch">Pitch</label>
        <input type="range" min="0" max="2" step="0.1" name="pitch" id="pitch" value="1">
    </div>

    <button id="speak">Speak</button>

</div>